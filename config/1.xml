<mixip>
  <serial>
    <tx>
      <name>lora1</name>
      <device>/dev/ttyUSB0</device>
      <driver>./libdefault.so</driver>
    </tx>
    <rx>
      <name>lora2</name>
      <device>/dev/ttyUSB1</device>
      <driver>./libdefault.so</driver>
    </rx>
  </serial>
  <network>
    <ring>
      <nframes>16</nframes>
    </ring>
    <default>
      <name>eth0</name>
    </default>
  </network>
</mixip>